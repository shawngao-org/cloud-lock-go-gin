# 服务器相关配置
server:
  # 服务器监听的IP地址
  ip: 127.0.0.1
  # 监听的端口号
  port: 8089
# 数据库相关配置
database:
  # MySQL
  mysql:
    # MySQL 主机地址
    host: 127.0.0.1
    # MySQL 端口(默认3306)
    port: 3306
    # 数据库名称
    db: ''
    # 用户
    user: ''
    # 密码
    password: ''
  # Redis
  redis:
    # Redis 主机地址
    host: 127.0.0.1
    # Redis 端口号(默认6379)
    port: 6379
    # 数据库序号(默认0)
    db: 0
    # Redis 密码(默认为空)
    password: ''
  influxdb:
    enable: true
    host: ''
    port: 8086
    token: ''
    org: ''
    bucket: ''
# 安全相关配置
security:
  # JWT
  jwt:
    # 加密 JWT 的私钥
    secret: ''
    # JWT 的过期时间(秒)
    timeout: 2592000
  # RSA
  rsa:
    # 公钥
    public: ''
    # 私钥
    private: ''
# NacOS 统一配置中心
nacos:
  # 是否启用统一配置
  enable: false
  # NacOS 主机地址
  ip: ''
  # NacOS 端口号(默认8848)
  port: 8848
  # 用户名
  username: ''
  # 密码
  password: ''
  # 命名空间
  namespace: ''
  # 配置文件所属组
  group: ''
  # 配置文件的数据编号
  dataId: ''
  # 超时时长
  timeout: 5000
  # 日志级别
  loglevel: debug
# 开发者模式(开发者模式会绕过所有的权限认证)
develop: false
